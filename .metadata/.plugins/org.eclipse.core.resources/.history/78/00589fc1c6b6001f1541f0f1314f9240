package LoopingStatement;

public class ForLoopProgram {
	
	public static void main(String[] args) {
			System.out.println(calculateFact());
	}
	
	public static int calculateFact()
	{
		int fact=1;
		if(fact==1)
		{
			return 1;
		}
		else
		{
			for (int i=1;i<5;i++)
			{
				fact *=i;
			}
			return fact;
		}
		
	}

}
